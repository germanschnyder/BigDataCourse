  <row Id="7799676" PostId="6608798" Score="0" Text="function array_key_relative($array, $current_key, $offset = 1) {&#xA;        // create key map&#xA;        $keys = array_keys($array);&#xA;        // find current key&#xA;        $current_key_index = array_search($current_key, $keys);&#xA;        // return desired offset, if in array, or false if not&#xA;        if(isset($keys[$current_key_index + $offset])) {&#xA;            return $keys[$current_key_index + $offset];&#xA;        }&#xA;        return false;&#xA;    } this one saved me ." CreationDate="2011-07-07T10:03:56.500" UserId="767431" />
