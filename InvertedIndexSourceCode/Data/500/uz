  <row Id="6716078" PostId="5832967" Score="0" Text="function readfile_chunked($filename, $retbytes=TRUE) {&#xA;        $chunk_size = 1024*1024;&#xA;        $buffer = '';&#xA;        $cnt =0;&#xA;        $handle = fopen($filename, 'rb');&#xA;        if ( $handle === false ) {return false;}&#xA;        stream_set_timeout($handle, 3600);&#xA;        while (!feof($handle)) {&#xA;          $buffer = fread($handle, $chunk_size );          &#xA;          echo $buffer;&#xA;          ob_flush();&#xA;          flush();&#xA;          if ($retbytes) { $cnt += strlen($buffer);}&#xA;        }&#xA;        $status = fclose($handle);&#xA;        if ($retbytes &amp;&amp; $status) {return $cnt;}&#xA;        return $status;&#xA;      }" CreationDate="2011-05-01T07:00:40.213" UserId="731143" />
