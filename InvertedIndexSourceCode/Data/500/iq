  <row Id="2906749" PostId="2860777" Score="0" Text="function myUrlEncode($string) {&#xA;	$entities = array('%21', '%2A', '%27', '%28', '%29', '%3B', '%3A', '%40', '%26', '%3D', '%2B', '%24', '%2C', '%2F', '%3F', '%25', '%23', '%5B', '%5D');&#xA;	$replacements = array('!', '*', &quot;'&quot;, &quot;(&quot;, &quot;)&quot;, &quot;;&quot;, &quot;:&quot;, &quot;@&quot;, &quot;&amp;&quot;, &quot;=&quot;, &quot;+&quot;, &quot;$&quot;, &quot;,&quot;, &quot;/&quot;, &quot;?&quot;, &quot;%&quot;, &quot;#&quot;, &quot;[&quot;, &quot;]&quot;);&#xA;	return str_replace($entities, $replacements, urlencode($string));&#xA;}" CreationDate="2010-05-19T01:23:42.160" UserId="343607" />
