  <row Id="1213106" PostId="1373295" Score="0" Text="Thank you, Mattias! &#xA;This works:&#xA;            AppDomain currentDomain = AppDomain.CurrentDomain;&#xA;            currentDomain.AssemblyResolve += new ResolveEventHandler(LoadFromSameFolderResolveEventHandler);&#xA;&#xA;        static Assembly LoadFromSameFolderResolveEventHandler(object sender, ResolveEventArgs args)&#xA;        {&#xA;            string folderPath = Path.GetDirectoryName(Assembly.GetExecutingAssembly().Location);&#xA;            string assemblyPath = Path.Combine(folderPath, args.Name + &quot;.dll&quot;);&#xA;            Assembly assembly = Assembly.LoadFrom(assemblyPath);&#xA;            return assembly;&#xA;        }" CreationDate="2009-09-03T16:03:52.823" UserId="129150" />
