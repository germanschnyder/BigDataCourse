  <row Id="5742386" PostId="5119849" Score="0" Text="SELECT TOP 1 WITH TIES *, b.GEOGraphy.STDistance(@input) AS dist FROM Numbers n JOIN UK b WITH(INDEX(spatial_geography_index)) -- index hint&#xA;		  ON b.Geography.STDistance(@input) &lt; @start*POWER(CAST(2 AS FLOAT),n.n)&#xA;		  AND b.Geography.STDistance(@input) &gt;= &#xA;			CASE WHEN n = 1 THEN 0 ELSE @start*POWER(CAST(2 AS FLOAT),n.n-1) END&#xA;			WHERE n &lt;= 10&#xA;			ORDER BY n		&#xA;		)SELECT TOP 1     @name=Name,    &#xA;	  FROM NearestNeighbor&#xA;	  ORDER BY n,dist" CreationDate="2011-02-25T17:21:22.943" UserId="98454" />
