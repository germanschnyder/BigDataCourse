  <row Id="1079769" PostId="1252903" Score="4" Text="It doesn't matter for the big O notation. It grows as 4^i = n. The 2 and 4 are just constants in this case" CreationDate="2009-08-10T02:43:45.950" UserId="105486" />
  <row Id="1079770" PostId="1252833" Score="0" Text="Thanks for answer !" CreationDate="2009-08-10T02:43:55.250" UserDisplayName="Hitcurst" />
