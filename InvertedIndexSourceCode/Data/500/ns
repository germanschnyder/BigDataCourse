  <row Id="4492752" PostId="4153387" Score="0" Text="select Data.ID, Data.MName, Names.FNames&#xA;from Data&#xA;    join &#xA;    (&#xA;        select MName, left(names, len(names) - 1) as FNames&#xA;        from Data as extern&#xA;            cross apply (select FName + ', '&#xA;                         from Data as intern&#xA;                         where extern.MName = intern.MName&#xA;                         for xml path('')&#xA;                        ) pre_trimmed (names)&#xA;        group by MName, names&#xA;    ) Names ON Data.MName = Names.MName&#xA;order by Data.ID" CreationDate="2010-11-12T04:43:11.980" UserId="504351" />
