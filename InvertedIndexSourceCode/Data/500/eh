  <row Id="1505941" PostId="1636449" Score="0" Text="public boolean equals(Object obj) {&#xA;    if (this == obj)&#xA;      return true;&#xA;    if (obj == null)&#xA;      return false;&#xA;    if (getClass() != obj.getClass())&#xA;      return false;&#xA;    Sync other = (Sync) obj;&#xA;    boolean smallerHash = System.identityHashCode(this) &lt; System.identityHashCode(other);&#xA;    synchronized (smallerHash?this:other) {&#xA;      synchronized (smallerHash?other:this) {&#xA;        if (i != other.i)&#xA;          return false;&#xA;      }&#xA;    }&#xA;    return true;&#xA;  }" CreationDate="2009-10-28T12:11:28.527" UserId="76024" />
